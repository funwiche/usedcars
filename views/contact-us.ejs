<section class="wpb-content-wrapper">
  <div
    data-vc-full-width="true"
    data-vc-full-width-init="true"
    data-vc-stretch-content="true"
    class="vc_row wpb_row vc_row-fluid vc_row-no-padding"
    style="
      position: relative;
      left: 15px;
      box-sizing: border-box;
      width: 657px;
      max-width: 657px;
    "
  >
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div class="wpb_wrapper">
          <div class="page-heading">
            <div class="page-title"><h2>Contact Us</h2></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="vc_row-full-width vc_clearfix"></div>
  <div class="container">
    <div class="vc_row wpb_row vc_row-fluid vc_custom_1559085036216">
      <div class="wpb_column vc_column_container vc_col-sm-8 col-sm-push-2">
        <div
          class="vc_column-inner vc_custom_1546420687080 klb_xs_vc_custom_1546420687083"
        >
          <div class="wpb_wrapper">
            <div class="klb-box">
              <div class="wpb_text_column wpb_content_element block-title">
                <div class="wpb_wrapper">
                  <h2>Contact Us</h2>
                </div>
              </div>
              <div
                class="wpcf7 js"
                id="wpcf7-f1122-p1111-o1"
                lang="en-US"
                dir="ltr"
              >
                <div class="screen-reader-response">
                  <p role="status" aria-live="polite" aria-atomic="true"></p>
                  <ul></ul>
                </div>
                <form class="wpcf7-form init" id="contact_form">
                  <p>
                    <label> Full Name</label>
                    <input class="text-field" required name="name" />
                  </p>
                  <div style="margin-bottom: 12px">
                    <label>Email Address </label>
                    <input
                      style="min-width: 100% !important"
                      class="text-field"
                      required
                      type="email"
                      name="email"
                    />
                  </div>
                  <p>
                    <label>Phone Number</label>
                    <input
                      class="text-field"
                      required
                      type="tel"
                      name="phone"
                    />
                  </p>

                  <p>
                    <label> Subject </label>
                    <input
                      class="text-field"
                      name="subject"
                      value="<%=subject%>"
                    />
                  </p>
                  <p>
                    <label> Your Message </label>
                    <textarea
                      cols="5"
                      rows="3"
                      required
                      class="text-field"
                      name="message"
                    ></textarea>
                  </p>
                  <p
                    id="error_output"
                    style="display: none"
                    class="alert alert-danger"
                  >
                    <span class="pl-1 pb-1"
                      >An error occured. Please try again later.</span
                    >
                  </p>
                  <p
                    id="success_output"
                    style="display: none"
                    class="alert alert-success"
                  >
                    <span class="pl-1 pb-1">Message sent succeffully!</span>
                  </p>
                  <p>
                    <input
                      class="wpcf7-form-control wpcf7-submit"
                      type="submit"
                      value="Submit"
                    />
                    <span id="submit_spinner" style="display: none">
                      <img src="/images/loader.gif" width="30" />
                    </span>
                  </p>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
